@font-face {
    font-family: 'Comfortaa-Light';
    src: url(<%= asset_path 'Comfortaa-Light.ttf' %>);
    font-weight: normal;
    font-style:normal;
}

@font-face {
    font-family: 'ProximaNovaBlRegular';
    src: url(<%= asset_path 'proximanova-black-webfont.eot' %>);
    src: url(<%= asset_path 'proximanova-black-webfont.eot?#iefix' %>) format('embedded-opentype'),
         url(<%= asset_path 'proximanova-black-webfont.woff' %>) format('woff'),
         url(<%= asset_path 'proximanova-black-webfont.ttf' %>) format('truetype');
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'ProximaNovaLtRegular';
    src: url(<%= asset_path 'proximanova-light-webfont.eot' %>);
    src: url(<%= asset_path 'proximanova-light-webfont.eot?#iefix' %>) format('embedded-opentype'),
         url(<%= asset_path 'proximanova-light-webfont.woff' %>) format('woff'),
         url(<%= asset_path 'proximanova-light-webfont.ttf' %>) format('truetype');
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'ProximaNovaLtItalic';
    src: url(<%= asset_path 'proximanova-lightit-webfont.eot' %>);
    src: url(<%= asset_path 'proximanova-lightit-webfont.eot?#iefix' %>) format('embedded-opentype'),
         url(<%= asset_path 'proximanova-lightit-webfont.woff' %>) format('woff'),
         url(<%= asset_path 'proximanova-lightit-webfont.ttf' %>) format('truetype');
    font-weight: normal;
    font-style: normal;

}

